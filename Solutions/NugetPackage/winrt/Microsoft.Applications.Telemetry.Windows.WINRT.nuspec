<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>Microsoft.Applications.Telemetry.Windows.WINRT</id>
        <version>0.0.0.0</version>
        <title>Microsoft.Applications.Telemetry.Windows.WINRT</title>
        <authors>Aria Engineering - Client SDK (ariaecsdk@microsoft.com)</authors>
        <projectUrl>https://aria.microsoft.com</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>A component to send telemetry data to ARIA from Windows 8.1+ applications</description>
        <copyright>Microsoft</copyright>
        <tags>"MS Internal Only"</tags>
    </metadata>
    <files>
        <file src="build\*.*"         target="build\netcore451" />
	<file src="build\*.*"         target="build\win81" />
        <file src="build\*.*"         target="build\wp81" />
        <file src="build\*.*"         target="build\wpa81" />

        <file src="lib\**\*.*"        target="lib" />
        <file src="lib\win81\**\*.*"  target="lib\netcore451" />
        <file src="lib\win81\**\*.*"  target="lib\win81" />
        <file src="lib\wp81\**\*.*"   target="lib\wp81" />
        <file src="lib\wp81\**\*.*"   target="lib\wpa81" />
    </files>
</package>